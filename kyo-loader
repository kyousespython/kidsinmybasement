local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Kyo Loader", colors)

local UniversalTab = Window:NewTab("Player")
local UniversalSection1 = UniversalTab:NewSection("Selection")


UniversalSection1:NewSlider("Walk Speed", "Changes ur Walk Speed.", 500, 16, function(s)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)


UniversalSection1:NewSlider("Jump Power", "Changes ur Jump Power.", 500, 50, function(jp)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = jp
end)

UniversalSection1:NewToggle("Toggle Jump Power", "Toggles Jump Power.", function(state)
    
    if state then
        game.Players.LocalPlayer.Character.Humanoid.UseJumpPower = true
    else
        game.Players.LocalPlayer.Character.Humanoid.UseJumpPower = false
    end
end)



