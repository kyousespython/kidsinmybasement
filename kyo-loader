local Version = "0.4"


local HomeTab = Window:NewTab("Credits")
local HomeSection = HomeTab:NewSection("Home")

HomeSection:NewLabel("Welcome, " .. game.Players.LocalPlayer.Name .. " to Kyos Loader")
HomeSection:NewLabel("Version is " .. Version .. ".")

HomeSection:NewLabel("Additional Help: <font color='rgb(0, 255, 255)'>kyo#2222</font>")
HomeSection:NewKeybind("Set Keybind", "Toggle UI", Enum.KeyCode.F, function()
	Library:ToggleUI()
end)
HomeSection:NewColorPicker("GUI color", "Color choose!", Color3.fromRGB(0,0,0), function(color)
    print(color)
    -- Second argument is the default color
end)
for theme, color in pairs(themes) do
    HomeSection:NewColorPicker(theme, "Change your "..theme, color, function(color3)
        Library:ChangeColor(theme, color3)
    end)
end
HomeSection:NewButton("Copy Discord Invite")
setclipboard("https://discord.gg/mEBPWnUCCd")
